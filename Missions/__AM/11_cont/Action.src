var tmp;
begin

// [90$00, 80$00, 75$00]
if tick < [70$00, 60$00, 55$00][Difficulty] then
   AddMedal('Time1', 1)
else if not powell_warn then
     AddMedal('Time1', -1)
    else
     AddMedal('Time1', -2);

