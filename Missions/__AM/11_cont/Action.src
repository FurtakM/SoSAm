   AddMedal('Destroy', -2)
  else
   if FilterAllUnits([[f_side, 2], [f_type, unit_building], [f_ok]]) >= 25 then
      AddMedal('Destroy', -1)
     else
      if FilterAllUnits([[f_side, 2], [f_type, unit_building], [f_ok]]) >= 15 then
         AddMedal('Destroy', 1)
        else
         AddMedal('Destroy', 2);

if artifact_get then
   AddMedal('Artefact', 1)
  else
   AddMedal('Artefact', -1);

GiveMedals('MAIN');


selected := UnitFilter(selected, [f_alive]);

// save
