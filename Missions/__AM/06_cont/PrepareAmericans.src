Export function PrepareJoan;
var i, j, tmp, rnd;
begin
    j = 0;

    repeat
        wait(0$1);
        for i in FilterAllUnits([[f_side, 1], [f_type, unit_human], [f_sex, 1], [f_or, [f_class, 1], [f_class, 2], [f_class, 3], [f_class, 4]]]) do
        begin
            if NOT FilterAllUnits([[f_side, 3], [f_or, [f_type, unit_human], [f_type, unit_vehicle], [f_type, unit_building], [f_occupied]], [f_dist, i, 25]]) AND GetDistUnits(i, JMM) < 15 then
            begin
                tmp = i;
                j = 1;
            end;
        end;

    until j = 1;
    
    blockSpawn = 1;
    uc_side = 4;

    Joan = PrepareUnit('Joan', false);
    PlaceUnitXYR(Joan, GetX(tmp), GetY(tmp), 5, true);
    SetLives(Joan, 250);

    InGameOn;
    CenterNowOnUnits(Joan);
    wait(0$2);
    ComExitVehicle(tmp);
    ComExitBuilding(tmp);
    wait(0$1);
    ComMoveUnit(tmp, Joan);
    wait(0$2);
    DialogueOn;

    if tmp = JMM then
    begin
        ComTurnUnit(JMM, Joan);
        Say(JMM, 'D5b-JMM-1');
        DialogueOff;
        SetLives(Joan, 500);
        ComTurnUnit(Joan, JMM);
        ComTurnUnit(JMM, Joan);
        wait(0$1);
        DialogueOn;
        Say(Joan, 'D5b-Joan-1');
        Say(JMM, 'D5b-JMM-2');
        Say(Joan, 'D5b-Joan-2');
        Say(JMM, 'D5b-JMM-3');
        Say(Joan, 'D5b-Joan-3');
        DialogueOff;
    end else
    begin
        ComTurnUnit(tmp, Joan);
        Say(tmp, 'D5a-Sol1-1');
        DialogueOff;
        SetLives(Joan, 500);
        ComTurnUnit(Joan, tmp);
        ComTurnUnit(tmp, Joan);
        wait(0$1);
        DialogueOn;
        Say(Joan, 'D5a-Joan-1');
        Say(tmp, 'D5a-Sol1-2');
        Say(Joan, 'D5a-Joan-2');
        Say(tmp, 'D5a-Sol1-3');
        Say(Joan, 'D5a-Joan-3');
        Say(tmp, 'D5a-Sol1-4');
        DialogueOff;

        ComExitVehicle(JMM);
        ComExitBuilding(JMM);
        wait(0$1);
        repeat
            wait(0$1);
            ComMoveUnit([Joan, tmp], JMM);
        until GetDistUnits(Joan, JMM) < 5;

        ComTurnUnit(Joan, JMM);
        ComTurnUnit(JMM, Joan);
        wait(0$1);
        DialogueOn;
        Say(Joan, 'D5a-Joan-4');
        Say(JMM, 'D5a-JMM-4');
        Say(Joan, 'D5a-Joan-5');
        Say(JMM, 'D5a-JMM-5');
        Say(Joan, 'D5a-Joan-6');
        DialogueOff;
    end;

    DialogueOn;
    Say(JMM, 'D6-JMM-1');
    Say(Joan, 'D6-Joan-1');
    Say(JMM, 'D6-JMM-2');
    Say(Joan, 'D6-Joan-2');
    Say(JMM, 'D6-JMM-3');
    Say(Joan, 'D6-Joan-3');
    Say(JMM, 'D6-JMM-4');

    DialogueOff;
    InGameOff;
    SetSide(Joan, 1);

    ChangeMissionObjectives('MJ');
    JoanStatus = 1;
    blockSpawn = 0;

end;

