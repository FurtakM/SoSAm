Every 0$1 do
var i;
begin
enable;

if not ignoreTarget then
   exit;

for i in ignoreTarget do
    if (FilterAllUnits([[f_side, 1], [f_dist, i, 10]]) diff ignoreTarget) > 1 then
       begin
       ignoreTarget := ignoreTarget diff i;
       break;
       end;
End;